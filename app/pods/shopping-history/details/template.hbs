<h1>Order details</h1>
<div class="row">
  <table class="table">
    <thead>
      <th>
        LineId
      </th>
      <th>
        Product
      </th>
      <th>
        Quantity
      </th>
    </thead>
    <tbody>
      {{#each model.lines as |line|}}
        <tr>
          <td>
            {{line.id}}
          </td>
          <td>
            {{line.product.name}}
          </td>
          <td>
            {{line.quantity}}
          </td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</div>
